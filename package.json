{
  "name": "vk-hacker-news",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tunnel": "vk-tunnel --insecure=1 --http-protocol=https --ws-protocol=wss --host=0.0.0.0 --port=10888",
    "deploy": "vite build && vk-miniapps-deploy",
    "clean-source": "rimraf README.md src webroot package.json"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "@tanstack/react-query": "^5.32.1",
    "@types/react-redux": "^7.1.33",
    "@vkontakte/icons": "^2.112.0",
    "@vkontakte/vk-bridge": "^2.13.0",
    "@vkontakte/vk-bridge-react": "^1.0.1",
    "@vkontakte/vk-mini-apps-router": "^1.4.2",
    "@vkontakte/vkui": "^6.0.0",
    "axios": "^1.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vkontakte/vk-miniapps-deploy": "^0.1.7",
    "@vkontakte/vk-tunnel": "^0.2.0",
    "eruda": "^3.0.1",
    "esbuild": "~0.20.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "terser": "^5.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
